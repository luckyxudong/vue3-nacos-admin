{
  "name": "vue3-nacos-admin",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:dev": "vite --mode dev",
    "dev:mock": "vite --mode mock",
    "dev:uat": "vite --mode uat",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:dev": "run-p type-check \"build-only {@} --mode dev\" --",
    "build:uat": "run-p type-check \"build-only {@} --mode uat\" --",
    "build:prod": "run-p type-check \"build-only {@} --mode prod\" --",
    "build:mock": "npm run build-only -- --mode mock",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint": "run-s lint:*",
    "lint:oxlint": "oxlint . --fix",
    "lint:eslint": "eslint . --fix --cache",
    "format": "prettier --write --experimental-cli src/"
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "codemirror": "5",
    "codemirror-editor-vue3": "^2.8.0",
    "diff-match-patch": "^1.0.5",
    "js-yaml": "^4.1.1",
    "lucide-vue-next": "^0.563.0",
    "monaco-editor": "^0.55.1",
    "monaco-editor-vue3": "^1.0.5",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "radix-vue": "^1.9.17",
    "tailwind-merge": "^3.4.0",
    "vue": "^3.5.26",
    "vue-i18n": "11",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.431",
    "@intlify/unplugin-vue-i18n": "^11.0.3",
    "@tsconfig/node24": "^24.0.4",
    "@types/codemirror": "^5.60.17",
    "@types/js-yaml": "^4.0.9",
    "@types/mockjs": "^1.0.10",
    "@types/node": "^24.10.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.3",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-oxlint": "~1.41.0",
    "eslint-plugin-vue": "~10.7.0",
    "jiti": "^2.6.1",
    "mockjs": "^1.1.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "~1.41.0",
    "prettier": "3.8.1",
    "sass": "^1.97.3",
    "typescript": "~5.9.3",
    "unocss": "^66.6.0",
    "unplugin-auto-import": "^21.0.0",
    "unplugin-vue-components": "^31.0.0",
    "unplugin-vue-router": "^0.19.2",
    "vite": "^7.3.1",
    "vite-plugin-mock": "^3.0.2",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^3.2.2"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
